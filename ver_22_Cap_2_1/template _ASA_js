if (!values.TITLU_R3_C31) {
    webform.errors.push({
        'fieldName': 'TITLU_R3_C31',
        'msg': Drupal.t('Cod eroare: A.09, Dacă nu sunt completate în antet Nr tel.  atunci - eroare critice')
    });
}


